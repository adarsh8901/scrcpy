   {
      case 1:{
	       int i,j,res=0;
	       printf("Enter a first number:-");
	       scanf("%d",&i);
	       printf("Enter second number:-");
	       scanf("%d",&j);
	       res=i+j;
	       printf("%d",res);
	   }
	 break;
